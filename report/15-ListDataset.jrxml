	<queryString>
		<![CDATA[SELECT
  c.`name` as `category`,
  f.`rating`,
  f.`film_id`,
  f.`title`,
  f.`description`,
  f.`rental_rate`,
  f.`last_update`,
  f.`release_year`
FROM
  film f INNER JOIN
  film_category fc ON (f.`film_id` = fc.`film_id`) INNER JOIN
  category c ON (c.`category_id` = fc.`category_id`)
ORDER BY 1, 2]]>
